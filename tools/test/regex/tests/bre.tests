[-a-c]12;a12z12b12u12c12-12;((0,3),(6,9),(12,15),(15,18))

# Collating elements
[[=a=]];bbbbabbbb;((4,5))

# Character classes
[[:alnum:]];__a____4_;((2,3),(7,8))
[[:alpha:]];012345a67890b;((6,7),(12,13))
[[:blank:]];0123456789abcdefghijklmnopqrstuvwxyz;()
[[:cntrl:]];012346789;()
