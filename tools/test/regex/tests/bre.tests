# Matching multiple character
#a*;aaaabbbaa;((0,4),(7,9))
a*b;aaaabbbaa;((0,5),(5,6),(6,7))
a\{3\};aaabbaabaaaa;((0,3),(8,11))
a\{1,3\};abbaaabaaaab;((0,1),(3,6),(7,10),(10,11))
a\{2,\};abbaabbaaab;((3,5),(7,10))

# Backreferences
\(a\)\1;aazz;((0,2))
\(a\)\1;aabbaazz;((0,2),(4,6))
