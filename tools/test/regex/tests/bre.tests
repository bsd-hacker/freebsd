#[[:graph:]];"     5   ";((5,6))
[[:lower:]];ABCDEFg0123456hIJKL;((6,7),(14,15))
#[[:print:]];
[[:punct:]];01234,6789.abcd:efgh_;((5,6),(10,11),(15,16),(20,21))
#[[:space:]];
[[:upper:]];abcdEfghIjkLmnoPqrstU;((4,5),(8,9),(11,12),(15,16),(20,21))
